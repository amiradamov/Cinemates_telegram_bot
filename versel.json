{
    "version": 2,
    "builds": [
      {
        "src": "bot.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bot.py"
      }
    ],
    "env": {
      "TELEGRAM_BOT_TOKEN": "@telegram_bot_token",
      "TMDB_API_KEY": "@tmdb_api_key"
    }
  }